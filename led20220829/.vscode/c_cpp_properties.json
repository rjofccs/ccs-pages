{
    "configurations": [
        {
            "name": "Win32",
            "browse": {
                "path": [
                    "${workspaceFolder}\\**",
                    "${workspaceFolder}\\User\\**",
                    "${workspaceFolder}\\Library\\**",
                    "${workspaceFolder}\\Start\\**",
                    "${workspaceFolder}\\System\\**",
                    "${workspaceFolder}\\User\\**"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}\\User\\**",
                "${workspaceFolder}\\Library\\**",
                "${workspaceFolder}\\Start\\**",
                "${workspaceFolder}\\System\\**",
                "${workspaceFolder}\\User\\**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "__CC_ARM",
                "USE_STDPERIPH_DRIVER",
                "STM32F10X_HD"
            ],
            "compilerPath": "D:\\Program Files\\LLVM\\bin\\clang.exe",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64"
        }
    ],
    "version": 4
}